(defsynth kick [amp 0.5 decay 0.6 freq 65]
  (let [env (env-gen (perc 0 decay) 1 1 0 1 FREE)
        snd (sin-osc freq (* Math/PI 0.5) amp)]
    (out 0 (pan2 (* snd env) 0))))
(kick)
    (out 0 (pan2 (* snd env) 0))))
